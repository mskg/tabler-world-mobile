{
  "name": "@mskg/tabler-world-backend",
  "author": "Markus Kling",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@sentry/node": "^5.4.0",
    "apollo-datasource-rest": "^0.5.0",
    "apollo-server": "^2.8.1",
    "apollo-server-caching": "^0.4.0",
    "apollo-server-lambda": "^2.8.1",
    "aws-xray-sdk": "^2.3.3",
    "casual": "^1.6.2",
    "dataloader": "^1.4.0",
    "date-and-time": "^0.7.0",
    "expo-server-sdk": "^3.3.0",
    "faker": "^4.1.0",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.4",
    "lambda-warmer": "^1.1.2",
    "lodash": "^4.17.11",
    "lru-cache": "^5.1.1",
    "pg": "^7.8.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.26",
    "@types/date-and-time": "^0.6.0",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.0",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^11.10.4",
    "@types/pg": "^7.4.13",
    "aws-sdk": "^2.414.0",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "file-loader": "^3.0.1",
    "ntl": "^3.2.4",
    "serverless": "^1.49.0",
    "serverless-domain-manager": "^3.2.6",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-iam-roles-per-function": "^2.0.1",
    "serverless-offline": "^5.8.0",
    "serverless-offline-ssm-provider": "^1.0.5",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-webpack": "^5.3.0",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.32.1",
    "webpack-node-externals": "^1.7.2"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test-export": "env-cmd --no-override ../config/.env cross-env-shell \"source ../config/.env.local && ts-node --project ./tsconfig.json scripts/export\"",
    "deploy": "env-cmd --no-override ../config/.env cross-env-shell \"serverless deploy --force --verbose\"",
    "package": "env-cmd --no-override ../config/.env cross-env-shell \"serverless package --force --verbose\"",
    "deploy-func:graphql": "env-cmd --no-override ../config/.env cross-env-shell \"serverless deploy function -f graphql --verbose\"",
    "deploy-func:reader": "env-cmd --no-override ../config/.env cross-env-shell \"serverless deploy function -f import-reader --verbose\"",
    "remove": "env-cmd --no-override ../config/.env cross-env-shell \"serverless remove --verbose\"",
    "dev": "env-cmd --no-override ../config/.env cross-env-shell \"source ../config/.env.local && serverless offline start --verbose --noTimeout --noAuth --skipCacheInvalidation\"",
    "dev-nocache": "env-cmd --no-override ../config/.env cross-env-shell \"source ../config/.env.local && serverless offline start --verbose --noTimeout --noAuth\"",
    "dynamodb": "env-cmd --no-override ../config/.env cross-env-shell \"source ../config/.env.local && serverless dynamodb start\"",
    "start": "ntl"
  }
}

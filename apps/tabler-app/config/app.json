{
    "expo": {
        "name": "ROUND TABLE Family.APP",
        "privacy": "unlisted",
        "sdkVersion": "40.0.0",
        "description": "'One community, one web' has evolved from a dream and a vision to a hard fact solution. All associations of the ROUND TABLE Family came together and gave a core team a great task to produce a solution that would empower all our members to connect online. You will be the first to adopt, adapt and improve on a administration platform as we take a giant leap forward â€“ Bringing Circling and Tabling into the 21st century!",
        "platforms": [
            "ios",
            "android"
        ],
        "extra": {
            "region": "",
            "identityPoolId": "",
            "userPoolId": "",
            "userPoolWebClientId": "",
            "api": "",
            "wsapi": "",
            "apidemo": "",
            "sentry": "",
            "cognitoAnalytics": "",
            "amplitudeAnalytics": "",
            "appLanguages": ""
        },
        "orientation": "portrait",
        "icon": "./assets/images/logo_iOS.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "enabled": true,
            "checkAutomatically": "ON_ERROR_RECOVERY",
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "assets/**/logo_iOS.png",
            "assets/**/logo_App_Logo.png",
            "assets/**/splash.png",
            "assets/**/background.png",
            "assets/**/logo_mask.png",
            "assets/**/icon_tw.png",
            "assets/fonts/**/DejaVuSansCondensed.ttf",
            "assets/fonts/**/DejaVuSansCondensed-Bold.ttf",
            "assets/fonts/**/DejaVuSans-ExtraLight.ttf",
            "assets/docs/**/*.md"
        ],
        "scheme": "tablerworld",
        "ios": {
            "userInterfaceStyle": "automatic",
            "supportsTablet": true,
            "config": {
                "usesNonExemptEncryption": false
            },
            "infoPlist": {
                "UIBackgroundModes": [
                    "fetch",
                    "location"
                ],
                "LSApplicationQueriesSchemes": [
                    "googlechrome",
                    "googlechromes",
                    "tg",
                    "sgnl",
                    "whatsapp",
                    "ms-outlook",
                    "googlegmail",
                    "http",
                    "https",
                    "tel",
                    "sms",
                    "comgooglemaps"
                ]
            }
        },
        "locales": {},
        "android": {
            "permissions": [
                "READ_CONTACTS",
                "WRITE_CONTACTS",
                "READ_CALENDAR",
                "WRITE_CALENDAR",
                "VIBRATE",
                "CAMERA",
                "ACCESS_COARSE_LOCATION",
                "ACCESS_FINE_LOCATION",
                "ACCESS_BACKGROUND_LOCATION",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE"
            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo_Android.png",
                "backgroundColor": "#ffffff"
            },
            "config": {
                "googleMaps": {}
            }
        },
        "androidNavigationBar": {
            "visible": "sticky-immersive"
        },
        "androidStatusBar": {
            "backgroundColor": "#ffffff00"
        },
        "notification": {
            "icon": "./assets/images/logo_Android_notifications.png",
            "color": "#ffbf35"
        }
    }
}
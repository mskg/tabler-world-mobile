    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "ntl",
        "bump-cli": "yarn add expo-cli --dev",
        "licenses": "license-checker --production --json | node config/3rdparty.js > ./assets/docs/licenses.md",
        "config:aws": "bash -c 'source ~/.bashrc && aws cloudformation list-exports > ./config/aws.json'",
        "config": "yarn run config:aws && yarn run config:app",
        "config:app": "run-env node config/create.js",
        "dev": "yarn expo start",
        "codegen": "rm -f src/model/graphql/*.ts && yarn apollo client:codegen --target=typescript --outputFlat src/model/graphql",
        "android": "emulator -avd expo -gpu host",
        "publish": "expo publish --release-channel dev",
        "translations": "ts-node --project config/tsconfig.json config/generate-translations"
    },
    "workspaces": {
        "packages": [
            "../../packages/tabler-world-tslint-rules",
            "../../packages/tabler-world-yarn-tools",
            "../../packages/tabler-world-geo-bigdata",
            "../../packages/tabler-world-config-app"
